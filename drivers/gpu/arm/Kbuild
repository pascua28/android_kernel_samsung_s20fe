ifeq ($(CONFIG_MALI_DDK_VERSION),y)
		ifeq ($(CONFIG_MALI_VALHALL_R20P0),y)
			obj-y += v_r20p0/
		endif
		ifeq ($(CONFIG_MALI_VALHALL_R25P1),y)
			obj-y += v_r25p1/
		endif
		ifeq ($(CONFIG_MALI_DDK_VALHALL_R32P1),y)
			obj-$(CONFIG_MALI_MIDGARD) += bv_r32p1/
		endif
		ifeq ($(CONFIG_MALI_DDK_VALHALL_R37P0),y)
			obj-$(CONFIG_MALI_MIDGARD) += bv_r37p0/
		endif
		ifeq ($(CONFIG_MALI_DDK_VALHALL_R38P0),y)
			obj-$(CONFIG_MALI_MIDGARD) += bv_r38p0/
		endif
		ifeq ($(CONFIG_MALI_DDK_VALHALL_R39P0),y)
			obj-$(CONFIG_MALI_MIDGARD) += bv_r39p0/
		endif
endif
